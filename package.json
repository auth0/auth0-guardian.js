    "semver": "^5.3.0",
    "sinon": "^1.17.6",
    "watch": "^1.0.1",
    "webpack": "^1.13.2"
  },
  "scripts": {
    "test": "npm run eslint && mocha --recursive test",
    "build": "webpack --config webpack.config.js",
    "build:clean": "rm -rf ./dist && mkdir dist",
